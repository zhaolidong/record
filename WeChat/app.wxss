.container {
	margin: 0;
	/*padding: 0;*/
	height:1204rpx;
	width:750rpx;
	position: relative;
	 background-color:#eeeffd;
	 overflow-y: scroll;
	 -webkit-overflow-scrolling: touch;
	 padding: 20rpx 0 100rpx;
	 box-sizing: border-box;
}
.cont{
	overflow: scroll;
}
/*导航*/
.nav{
	position: absolute;
	left:0;
	bottom:0;
	width:100%;
  z-index: 100;

}
navigator{
  color: #fff;
  font-size: 30rpx;
  height: 98rpx;
  line-height: 98rpx;
  background-color: #edb5b6;
  box-sizing:border-box;
	width:25%;
  display: inline-block;
  text-align: center;
  border-right: 1px solid #ee999b;
}
navigator:last-child{
  border-right: none;
}
/*轮播图*/
.swiper{
  width: 96%;
  height: 292rpx;
  margin: auto;
  border-radius: 8rpx;
}
.swiper image{
  width: 100%;
  height: 100%;
}
